{
  "extends": "airbnb-base",
  "rules": {
    "max-len": ["error", 120],
    "no-param-reassign": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": false
      }
    ],
    "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"]
  },
  "env": {
    "jest": true
  }
}
